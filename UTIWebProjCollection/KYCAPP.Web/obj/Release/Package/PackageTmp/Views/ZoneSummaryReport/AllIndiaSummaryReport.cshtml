    <input type="hidden" value="" name="p_aum_bracket" />
    <input type="hidden" name="download" value="true" />
</form>
<script type="text/javascript">


    function close_btn() {
        $(".modal").css("display", "none");
    }

    $('#btnExportExcel').click(function () {
        let REPORT_DT = $('#report_date_id').val();
        let AUM_BRACKET = $('#aum_bracket_id').val();
        let split_AUM_BRACKET = AUM_BRACKET.split("string:");
        if (split_AUM_BRACKET[1] == "" || split_AUM_BRACKET[1] == null) {
            split_AUM_BRACKET[1] = "All";
        }
        $("#exportexcel_form input[name=report_date]").val(REPORT_DT);
        $("#exportexcel_form input[name=p_aum_bracket]").val(split_AUM_BRACKET[1]);
        $('#exportexcel_form').submit();
    });

</script>
